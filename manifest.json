{
  "name": "Cross Navigations",
  "version": "0.0.1",
  "description": "Feature-dedicated navigations across various pages",
  "permissions": ["activeTab"],
  "icons": {
    "16": "images/get_started16.png",
    "32": "images/get_started32.png",
    "48": "images/get_started48.png",
    "128": "images/get_started128.png"
  },
  "manifest_version": 2,
  "background": {
    "persistent": false,
    "scripts": ["js/background.js", "lib/jquery-3.4.1.min.js"]
  },
  "commands": {
    "Focus capital input": {
      "description": "Focus the capital input of the active page",
      "suggested_key": {
        "default": "Alt+F"
      }
    }
  }
}
